{
  "expo": {
    "name": "Theraklick",
    "slug": "theraklick-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F0FDF4"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.theraklick.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F0FDF4"
      },
      "package": "com.theraklick.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#16A34A",
          "defaultChannel": "default"
        }
      ],
      [
        "@config-plugins/react-native-webrtc",
        {
          "cameraPermission": "Theraklick needs camera access for video calls",
          "microphonePermission": "Theraklick needs microphone access for calls"
        }
      ]
    ]
  }
}
